<p class="text-center dashboard-title">Add new event</p><br />
<div class="row">
  <div class="span6 card-background card-center">
  <%= form_for @event, html: { id: "multiselectForm" } do |f| %>
    <div class="form-group">
      <%= f.label :name %><br />
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :date %><br />
      <%= f.datetime_select(:date) %>
    </div>
    <div class="form-group">
      <%= f.select :user_ids, @users, {}, { multiple: true, class: 'form-control' }%>
    </div>
    <%= f.submit "Create", class: 'btn btn-primary' %>
  <% end %>
  </div>
</div>
