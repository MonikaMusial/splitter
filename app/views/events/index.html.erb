<%= link_to '+ Add Event', new_event_path, class: 'btn btn-primary' %>
<p>
<% @events.each do |event| %>
  <%= event.name %>
  <%= event.date %>
  <%= event.image_url %>
  <%= event.owner.nick %>
<% end %>
</p>

<p>
<% @owned_events.each do |event| %>
  <%= event.name %>
  <%= event.date %>
  <%= event.image_url %>
  <%= event.owner.nick %>
<% end %>
</p>
